{
  "kind": "collectionType",
  "collectionName": "categories",
  "info": {
    "singularName": "category",
    "pluralName": "categories",
    "displayName": "Category"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string"
    },
    "scores": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::score.score",
      "mappedBy": "category"
    },
    "weight": {
      "type": "decimal"
    },
    "active": {
      "type": "boolean",
      "default": false
    },
    "segment": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::segment.segment",
      "inversedBy": "categories"
    }
  }
}
