{
  "kind": "collectionType",
  "collectionName": "judge_requests",
  "info": {
    "singularName": "judge-request",
    "pluralName": "judge-requests",
    "displayName": "Judge Request"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "event": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::event.event",
      "inversedBy": "judge_request"
    },
    "judge": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::judge.judge",
      "inversedBy": "judge_request"
    },
    "request_status": {
      "type": "enumeration",
      "enum": [
        "approved",
        "pending",
        "rejected"
      ]
    }
  }
}
